<div class="container">
    <div class="row">
                <div class="panel panel-primary" style="margin-top: 20px;">
            <h2 class="pull-left"> Lista de notificaciones </h2> 
            <%= link_to 'Nuevo mensaje de notificación', new_notification_post_path ,class: "pull-right site_btn", remote: true %>
            <%#= link_to 'Nuevo mensaje de notificación', new_notification_post_path ,class: "pull-right site_btn" %>
                
             <div class="clearfix"> </div>   

                
                <div class="panel-body">
                    <ul class="list-group">
                    <% @notification_posts.each do |notification_post| %>
                        <li class="list-group-item">
                            <div class="checkbox">
                               
                                <label for="checkbox">
                                    <%= notification_post.name %>
                                </label>
                            </div>
                            <div class="pull-right action-buttons">
                                 <%= link_to notification_post do  %>
                                    <span class="glyphicon glyphicon-menu-hamburger Try it"></span>
                                 <% end %>
                                  <%= link_to edit_notification_post_path(notification_post), remote: true do %>
                                   <span class="glyphicon glyphicon-pencil"></span>
                                  <% end %>
                                 <%= link_to notification_post, method: :delete, data: { confirm: 'Are you sure?' } do %>
                                     <span class="glyphicon glyphicon-trash"></span>
                                 <% end %>
                              
                            </div>
                        </li>
                      <% end %>
                    </ul>
                </div>
                <div class="panel-footer">
                    <div class="row">
                        <div class="col-md-6">
                            <h6>
                                Total Count <span class="label label-info"><%= @notification_posts.size %></span></h6>
                        </div>
                        <div class="col-md-6">
                            <ul class="pagination pagination-sm pull-right">
                                  <%= paginate @notification_posts %>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
    </div>
</div>

